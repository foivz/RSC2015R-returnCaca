<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }}
      method="POST"
      class="fos_user_registration_register">

    {{ form_widget(form) }}

    <div>
        <input type="submit"
               class="btn btn-lg btn-primary"
               value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}"/>
    </div>
    <a href="{{ url('fos_user_security_login') }}">Already Have account?</a>
</form>
