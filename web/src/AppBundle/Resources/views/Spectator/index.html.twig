            <script src="{{ asset('bundles/app/js/spectator.js') }}"></script>
            <script src="{{ asset('bundles/app/js/richmarker.js') }}"></script>
        {% endblock %}

        {% block headEnd %}
            {{ parent() }}
            <title>Spectator - Blitz</title>
            <script src="{{ asset('bundles/app/js/wow.min.js') }}"></script>
            <script>
                new WOW().init();
            </script>
        {% endblock %}


